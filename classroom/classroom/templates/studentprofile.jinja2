
{% extends "studentnav.jinja2" %}
{% block content %}
<div>
<div class="container">
<h2> Profile</h2>
<form action="/studentedit" method="POST">

  <div class="form-group"> Name : {{name}}</div>
  <div class="form-group"> Student ID : {{session['sid']}}</div>
  <div class="form-group"> Institution name: {{ins}} </div>
  <div class="form-group"> Mail id: {{email}} </div>

  <button type="submit" class="btn btn-default">Edit</button>

</form>
</div>
</div>
{% endblock content %}
