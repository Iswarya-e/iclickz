{% extends "studentnav.jinja2" %}
{% block content %}





<table style="width:100%">
  <tr>
    <th>Course ID</th>
    <th>Course name</th>

  </tr>
{%for i in course :%}
  <tr>

    <td> {{i.course_id}}</td>
    <td> {{i.course_name}}</td>
    <td><form action="/studentfinalreport" method="POST">
      <input type="hidden" name ="course_id" value={{i.course_id}}>

      <input type="submit" value="View report"></form>
    </td>

  </tr>
{% endfor %}

</table>




{% endblock content %}
