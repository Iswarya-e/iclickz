            <div class="w3-container w3-center">

              <div class="container">
                <h2> Teacher Registration</h2>
                <form action="/t1">
                  <div class="form-group">
                    <label for="name">Name:</label>
                    <input type="name" class="form-control" id="name3" placeholder="Enter name" name="name">
                  </div>
                  <div class="form-group">
                    <label for="Institution name">Enter the Institution Name:</label>
                    <input type="Institution name" class="form-control" id="name4" placeholder="Enter the institution name" name="insname">
                  </div>
                  <div class="form-group">
                    <label for="Domain name">Enter the Domain Name:</label>
                    <input type="Domainname" class="form-control" id="name5" placeholder="Enter the domain name" name="domname">
                  </div>
                  <div class="form-group">
                    <label for="email">Email:</label>
                    <input type="email" class="form-control" id="email" placeholder="Enter email" name="email">
                    <label> {{ error1 }} </label>
                  </div>
                  <div class="form-group">
                    <label for="pwd">Password:</label>
                    <input type="password" class="form-control" id="pwd" placeholder="Enter password" name="pwd" title="Must contain atleast One number and one uppercase and lower case letter and atleast 8 or more characters" required >
                    <label> {{ error2 }} </label><br>

                    <input type="checkbox" id="2"onclick="myFunction()"><label for="2">Show Password</label><br>
                  </div>
                <div class="form-group">
                    <label for="pwd">Repeat Password</label>
                    <input type="password" class="form-control" id="rppwd" placeholder="Enter password" name="rppwd">
                    <label> {{ error3 }} </label><br>

                    <input type="checkbox" id="1" onclick="myFunction1()"><label for="1">Show Password</label>
                  </div>
                  <button type="submit" class="btn btn-default">Submit</button>
                  <a href="/teacherlogin">Already registered?</a>
                </form>
              </div>
            </div>
